    "headless Chrome",
    "NodeJS",
    "Express"
  ],
  "author": "Karl Schipul",
  "license": "ISC",
  "dependencies": {
    "chromedriver": "^87.0.4",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "filenamify-url": "^2.1.1",
    "nodemon": "^2.0.6",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "winston": "^3.3.3"
  },
  "type": "module"
}
